{
  "name": "inceptum",
  "version": "0.2.0",
  "description": "hipages take on the foundational library for enterprise-grade apps written in NodeJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "NODE_ENV=test nyc mocha",
    "itest": "NODE_ENV=test mocha itest",
    "lint": "node node_modules/tslint/bin/tslint -p . --type-check",
    "docs": "jsdoc -c jsdocconf.json -t ./node_modules/ink-docstrap/template -R README.md -r -d docs/api"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hipages/inceptum.git"
  },
  "engine": {
    "node": ">6.0.0"
  },
  "files": [
    "src/**/*.js"
  ],
  "author": "",
  "license": "Apache-2.0",
  "directories": {
    "lib": "dist",
    "test": "test"
  },
  "dependencies": {
    "@types/bunyan": "0.0.36",
    "body-parser": "^1.15.2",
    "bunyan": "^1.8.5",
    "bunyan-prettystream": "0.1.3",
    "bunyan-redis": "0.1.4",
    "bunyan-rotating-file-stream": "1.6.2",
    "codacy-coverage": "^2.0.0",
    "config": "1.25.1",
    "deep-assign": "2.0.0",
    "dtrace-provider": "^0.8.1",
    "express": "^4.14.0",
    "express-jwt": "^3.4.0",
    "express-yields": "^1.0.0",
    "json-schema-deref-sync": "^0.3.1",
    "json-stringify-safe": "5.0.1",
    "locutus": "^2.0.5",
    "lodash": "^4.17.4",
    "module-alias": "2.0.0",
    "moment": "^2.15.1",
    "mysql": "^2.11.1",
    "node-dogstatsd": "0.0.6",
    "nyc": "^10.3.2",
    "prom-client": "7.1.0",
    "redis": "2.6.5",
    "source-map-support": "^0.4.15",
    "striptags": "^2.1.1",
    "swagger-tools": "0.10.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/bunyan-prettystream": "0.1.30",
    "@types/mysql": "0.0.33",
    "@types/node": "7.0.23",
    "@types/source-map-support": "^0.2.28",
    "co-mocha": "^1.1.2",
    "doubleagent": "^1.1.0",
    "husky": "^0.11.6",
    "ink-docstrap": "1.3.0",
    "istanbul": "0.4.4",
    "jsdoc": "3.4.3",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "mocha-typescript": "1.1.2",
    "must": "^0.13.2",
    "sinon": "1.17.7",
    "ts-node": "3.0.4",
    "tslint": "^5.4.3",
    "tslint-config-shopify": "^2.0.0",
    "typescript": "2.3.2"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
