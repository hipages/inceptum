{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
	"include": [ "src" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "systemName": "Inceptum - Enterprise Node.js Framework",
        "theme": "united",
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
